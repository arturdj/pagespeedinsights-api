{
  "name": "azion-pagespeed-analyzer-ts",
  "version": "1.0.0",
  "description": "TypeScript implementation of Azion PageSpeed Analyzer with API endpoints",
  "type": "module",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "dev": "ts-node src/server.ts",
